#include<studio.h>
#include<string.h>
int main()
{ 
  Char str[]="abcd";
  Substirng(str,strlen(str));
  return(0);
}
Void substring(charstr[],int n)
{
  int x,i,k,j;
  for(i=1;i<=n;i++)
  {
    for(j=0;j<=n-i;j++)
    {
      int x=j+i;
      for(k=j;k<x;k++)
      Printf("%3c",str[k]);
      Printf("\n");
    }
   }
 }
